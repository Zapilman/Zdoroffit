{
  "expo": {
    "name": "Zdoroffit",
    "slug": "Zdoroffit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "backgroundColor": "#6780c5",
    "splash": {
      "image": "./assets/new_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#6780c5"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/RobotoRegular",
            "./assets/fonts/RobotoBold.ttf",
            "./assets/fonts/LXGWWenKaiTC-Bold.ttf",
            "./assets/fonts/LXGWWenKaiTC-Regular.ttf"
          ]
        }
      ]
    ],
    "scheme": "zdoroffit"
  }
}
